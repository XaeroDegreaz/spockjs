{
  "private": true,
  "name": "babel-plugin-spock",
  "version": "0.1.0",
  "description": "Structured JS test cases, inspired by Spock Framework",
  "keywords": [
    "spock",
    "framework",
    "test",
    "structure",
    "babel",
    "plugin"
  ],
  "main": "index.js",
  "author": {
    "name": "Tim Seckinger",
    "email": "seckinger.tim@gmail.com",
    "url": "https://github.com/jeysal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeysal/babel-plugin-spock"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage dist",
    "all": "run-p lint typecheck test-once build",
    "lint": "tslint --project .",
    "typecheck": "tsc",
    "test-once": "jest --no-cache --coverage --config jest.config.js",
    "test-watch": "jest --watchAll --coverage --config jest.config.js",
    "servecov": "live-server coverage/lcov-report --port=1717 -q",
    "test": "run-p --race test-watch servecov",
    "build": "babel src -d dist --extensions .ts",
    "postbuild": "run-p copymeta",
    "copymeta": "cp package.json LICENSE README.* dist",
    "prefix": "prettier --write {src,test}/**/*.ts *.md",
    "fix": "tslint --fix --project ."
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "babel-plugin-espower": "2.3.2"
  },
  "peerDependencies": {
    "babel-core": "^6.0.0",
    "power-assert": "^1.4.4"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.32",
    "@babel/core": "7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.32",
    "@babel/preset-env": "7.0.0-beta.32",
    "@babel/preset-typescript": "7.0.0-beta.32",
    "@types/babel-core": "^6.25.3",
    "@types/babel-traverse": "^6.25.2",
    "@types/babel-types": "^6.25.1",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "babel-core": "7.0.0-bridge.0",
    "babel-core-old": "npm:babel-core@^6.0.0",
    "babel-jest": "21.3.0-beta.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-types": "^6.26.0",
    "copyfiles": "^1.2.0",
    "jest": "^21.2.1",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.4",
    "prettier": "^1.3.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.1"
  }
}
